#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int n;
        cin>>n;
        unordered_map<int,int>mp;
        for(int i=0; i<n; i++)
        {
            int x;
            cin>>x;
            mp[x]++;
        }
        vector<int>v;
        for(auto it:mp)
        {
        v.push_back(it.second);
        }
        sort(v.begin(),v.end());
        int ans=0;
        int n1=v.size();
        for(int i=0;i<v.size();i++)
        {
         ans=max(ans,v[i]*(n1-i));
        }
        cout<<ans<<endl;
        
    }
}