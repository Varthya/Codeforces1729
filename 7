#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int n;
        cin>>n;
        long long int sum1=0;
        long long int sum2=0;
        for(int i=0; i<n; i++)
        {
            long long int x;
            cin>>x;
            if(x>=0)
            {
            sum1+=x;
            }
            else
            {
            sum2+=x;
            }

        }
        cout<<abs(sum1-abs(sum2))<<endl;
    }
}